{
	"name": "Yami",
	"type": "Dark",
	"colors": {
		"button.background": "#3E4351",
		"button.foreground": "#F3F4F6",
		"button.hoverBackground": "#353945",
		"checkbox.background": "#3E4351",
		"checkbox.foreground": "#F3F4F6",
		"checkbox.border": "#23262E",
		"settings.dropdownBackground": "#353945",
		"settings.dropdownForeground": "#F3F4F6",
		"settings.dropdownBorder": "#23262E",
		"editor.background": "#272A33",
		"panel.background": "#272A33",
		"editorCursor.foreground": "#F3F4F6",
		"editor.foreground": "#F3F4F6",
		"editor.lineHighlightBackground": "#23262E",
		"editorWhitespace.foreground": "#474C5C",
		"editor.selectionBackground": "#62697F",
		"sideBar.background": "#272A33",
		"sideBar.foreground": "#F3F4F6",
		"sideBar.dropBackground": "#23262E",
		"sideBarSectionHeader.background": "#23262E",
		"input.background": "#23262E",
		"list.activeSelectionBackground": "#23262E",
		"sideBarSectionHeader.foreground": "#F3F4F6",
		"activityBar.background": "#23262E",
		"activityBar.foreground": "#F3F4F6",
		"editorGroupHeader.tabsBackground": "#23262E",
		"titleBar.activeForeground": "#F3F4F6",
		"titleBar.activeBackground": "#23262E",
		"tab.inactiveBackground": "#353945",
		"tab.inactiveForeground": "#F3F4F6",
		"statusBar.background": "#23262E",
		"statusBar.foreground": "#F3F4F6",
		"statusBar.border": "#23262E",
		"statusBar.debuggingBackground": "#23262E",
		"statusBar.debuggingBorder": "#23262E",
		"statusBar.debuggingForeground": "#F3F4F6",
		"statusBar.noFolderBackground": "#272A33",
		"statusBar.noFolderForeground": "#F3F4F6",
		"statusBarItem.prominentBackground": "#ffca7a",
		"statusBarItem.prominentHoverBackground": "#ffee7a"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#6A728A",
				"fontStyle": " italic"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#85B6FF"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#EFA9EF"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#EFA9EF"
			}
		},
		{
			"name": "User-defined constant",
			"scope": ["constant.character", "constant.other"],
			"settings": {
				"foreground": "#EFA9EF"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": "",
				"foreground": "#39FFBA"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#FF476E"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#FF476E"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#01F7F7"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.class",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#39FFBA"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#FFEE7A"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#FF476E"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFEE7A"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#FF476E"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#39FFBA"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#39FFBA"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#FFEE7A"
			}
		},
		{
			"name": "Library class/type",
			"scope": ["support.type", "support.class"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#39FFBA"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": "",
				"foreground": "#FFEE7A"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"background": "#FF476E",
				"fontStyle": "",
				"foreground": "#F8F8F0"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#9470FF",
				"foreground": "#F8F8F0"
			}
		},
		{
			"name": "Invalid illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#9470FF",
				"foreground": "#F8F8F0"
			}
		},
		{
			"name": "JS Incidentals",
			"scope": [
				"source.js meta.brace.curly.js",
				"source.js meta.function.js punctuation.definition.parameters.begin.js",
				"source.js meta.function.js punctuation.definition.parameters.end.js",
				"source.js meta.brace.round.js",
				"source.js meta.delimiter.object.comma.js"
			],
			"settings": {
				"foreground": "#474C5C"
			}
		},
		{
			"name": "Python Incidentals",
			"scope": [
				"source.python meta.class.python punctuation.definition.inheritance.begin.python",
				"source.python meta.class.python punctuation.definition.inheritance.end.python",
				"source.python meta.function.python punctuation.definition.parameters.begin.python",
				"source.python meta.function.python punctuation.definition.parameters.end.python",
				"source.python meta.function-call.python punctuation.definition.arguments.begin.python",
				"source.python meta.function-call.python punctuation.definition.arguments.end.python",
				"source.python meta.function.python meta.function.parameters.python punctuation.separator.parameters.python"
			],
			"settings": {
				"foreground": "#474C5C"
			}
		},
		{
			"name": "Ruby Incidentals",
			"scope": [
				"source.ruby meta.function.method.with-arguments.ruby punctuation.definition.parameters.ruby",
				"source.ruby punctuation.section.array.ruby",
				"source.ruby punctuation.section.function.ruby",
				"source.ruby meta.require.ruby meta.environment-variable.ruby",
				"source.ruby punctuation.separator.variable.ruby"
			],
			"settings": {
				"foreground": "#474C5C"
			}
		},
		{
			"name": "Markup: strike",
			"scope": ["markup.strike"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffca7a"
			}
		},
		{
			"name": "Markup: link",
			"scope": ["meta.link.inline", "meta.link.reference"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#01f7f7"
			}
		},
		{
			"name": "Markup: bold",
			"scope": ["markup.bold"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffca7a"
			}
		},
		{
			"name": "Markup: italic",
			"scope": ["markup.italic"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffca7a"
			}
		},
		{
			"name": "Markdown: heading",
			"scope": ["markup.heading"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#39ffba"
			}
		},
		{
			"name": "Markdown: List Items Punctuation",
			"scope": ["punctuation.definition.list_item.markdown"],
			"settings": {
				"foreground": "#EFA9EF"
			}
		},
		{
			"name": "Markdown: Blockquote",
			"scope": ["markup.quote"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#85B6FF"
			}
		},
		{
			"name": "Markdown: Blockquote Punctuation",
			"scope": ["punctuation.definition.blockquote.markdown"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#85B6FF"
			}
		},
		{
			"name": "Markdown: Separator",
			"scope": ["meta.separator"],
			"settings": {
				"foreground": "#858db7"
			}
		},

		{
			"name": "Markdown: raw inline",
			"scope": ["text.html.markdown markup.raw.inline"],
			"settings": {
				"foreground": "#39ffba"
			}
		},

		{
			"name": "Markdown: underline",
			"scope": ["text.html.markdown markup.raw.inline"],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#ffca7a"
			}
		},
		{
			"name": "Markdown: Raw block",
			"scope": ["markup.raw.block"],
			"settings": {
				"foreground": "#39ffba"
			}
		},
		{
			"name": "Markdown: Raw Block fenced source",
			"scope": ["markup.raw.block.fenced.markdown source"],
			"settings": {
				"foreground": "#F3F4F6"
			}
		},
		{
			"name": "Markdown: Fenced Code Block",
			"scope": ["punctuation.definition.fenced.markdown", "variable.language.fenced.markdown"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#858db7"
			}
		},
		{
			"name": "Markdown: Fenced Language",
			"scope": ["variable.language.fenced.markdown"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#858db7"
			}
		},
		{
			"name": "Punctuation Accessor",
			"scope": ["punctuation.accessor"],
			"settings": {
				"foreground": "#EFA9EF"
			}
		},
		{
			"name": "Meta Function Return Type",
			"scope": ["meta.function.return-type"],
			"settings": {
				"foreground": "#EFA9EF"
			}
		},
		{
			"name": "Punctuation Section Block Begin",
			"scope": ["punctuation.section.block.begin"],
			"settings": {
				"foreground": "#F3F4F6"
			}
		},
		{
			"name": "Punctuation Section Block End",
			"scope": ["punctuation.section.block.end"],
			"settings": {
				"foreground": "#F3F4F6"
			}
		},
		{
			"name": "Punctuation Section Embedded Begin",
			"scope": ["punctuation.section.embedded.begin"],
			"settings": {
				"foreground": "#EFA9EF"
			}
		},
		{
			"name": "Punctuation Section Embedded End",
			"scope": ["punctuation.section.embedded.end"],
			"settings": {
				"foreground": "#EFA9EF"
			}
		},
		{
			"name": "Punctuation Separator Namespace",
			"scope": ["punctuation.separator.namespace"],
			"settings": {
				"foreground": "#EFA9EF"
			}
		},
		{
			"name": "Variable Function",
			"scope": ["variable.function"],
			"settings": {
				"foreground": "#39ffba"
			}
		},
		{
			"name": "Variable Other",
			"scope": ["variable.other"],
			"settings": {
				"foreground": "#F3F4F6"
			}
		},
		{
			"name": "Variable Language",
			"scope": ["variable.language"],
			"settings": {
				"foreground": "#EFA9EF"
			}
		},
		{
			"name": "Entity Name Module Ruby",
			"scope": ["entity.name.module.ruby"],
			"settings": {
				"foreground": "#85B6FF"
			}
		},
		{
			"name": "Entity Name Constant Ruby",
			"scope": ["entity.name.constant.ruby"],
			"settings": {
				"foreground": "#EFA9EF"
			}
		},
		{
			"name": "Support Function Builtin Ruby",
			"scope": ["support.function.builtin.ruby"],
			"settings": {
				"foreground": "#F3F4F6"
			}
		},
		{
			"name": "Storage Type Namespace CS",
			"scope": ["storage.type.namespace.cs"],
			"settings": {
				"foreground": "#EFA9EF"
			}
		},
		{
			"name": "Entity Name Namespace CS",
			"scope": ["entity.name.namespace.cs"],
			"settings": {
				"foreground": "#85B6FF"
			}
		}
	]
}
